{
  "name": "code-katas",
  "version": "1.0.0",
  "description": "A repository for challenges from Code Wars",
  "main": "index.ts",
  "scripts": {
    "run": "ts-node katas/index.ts",
    "format": "prettier --write \"katas/**/*.ts\" \"tests/**/*.ts\"",
    "format:check": "prettier --check \"katas/**/*.ts\" \"tests/**/*.ts\"",
    "test": "jest",
    "lint": "eslint \"katas/**/*.ts\" \"tests/**/*.ts\"",
    "lint:fix": "eslint \"katas/**/*.ts\" \"tests/**/*.ts\" --fix"
  },
  "author": "p-stanescu",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@typescript-eslint/eslint-plugin": "^8.23.0",
    "@typescript-eslint/parser": "^8.23.0",
    "eslint": "^9.19.0",
    "eslint-config-prettier": "^10.0.1",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "ts-jest": "^29.2.5"
  }
}
